{
  "name": "teamcity-project-status",
  "version": "1.0.0",
  "private": true,
  "config": {
    "components": "./src",
    "dist": "./dist"
  },
  "scripts": {
    "create-component": "yo @jetbrains/ring-ui:react --path $npm_package_config_components",
    "build": "webpack -p --devtool sourcemap",
    "dist": "npm run build && rm -f teamcity-project-status-widget.zip && zip -r -j teamcity-project-status-widget.zip ./dist/*.{js,json,html}",
    "test": "karma start",
    "lint": "eslint . && npm run stylelint",
    "stylelint": "stylelint --ignore-path .gitignore **/*.css",
    "ci-test": "npm run lint && xvfb-maybe karma start --teamcity",
    "start": "webpack-dev-server -d --history-api-fallback --inline --hot --colors --port 9010",
    "i18n:extract": "angular-gettext-cli --files \"{src/**/*.js,node_modules/hub-dashboard-addons/components/**/*.js}\" --dest src/app/translations/default.pot --marker-name=i18n"
  },
  "babel": {
    "presets": "@jetbrains/jetbrains"
  },
  "dependencies": {
    "@jetbrains/babel-preset-jetbrains": "1.1.1",
    "@jetbrains/hub-widget-ui": "^1.0.8",
    "@jetbrains/icons": "1.18.5",
    "@jetbrains/logos": "1.0.3",
    "@jetbrains/ring-ui": "^1.0.0-beta.181",
    "angular-gettext-cli": "1.2.0",
    "angular-gettext-loader": "1.0.1",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
    "babel-polyfill": "6.26.0",
    "css-loader": "1.0.0",
    "file-loader": "1.1.11",
    "html-loader": "0.5.5",
    "html-webpack-plugin": "3.2.0",
    "hub-dashboard-addons": "0.1.0",
    "postcss-cssnext": "3.1.0",
    "postcss-import": "^12.0.0",
    "postcss-loader": "3.0.0",
    "postcss-modules-values-replace": "3.0.1",
    "prop-types": "15.6.2",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-redux": "^5.0.7",
    "redbox-react": "1.6.0",
    "redux": "^4.0.0",
    "redux-act": "^1.7.4",
    "redux-thunk": "^2.3.0",
    "webpack": "3.12.0",
    "webpack-dev-server": "2.11.2"
  },
  "devDependencies": {
    "@jetbrains/generator-ring-ui": "1.0.0-beta.160",
    "@jetbrains/eslint-config": "2.0.0",
    "@jetbrains/stylelint-config": "1.3.3",
    "babel-eslint": "8.2.6",
    "chai": "4.1.2",
    "chai-as-promised": "7.1.1",
    "chai-dom": "1.8.0",
    "chai-enzyme": "1.0.0-beta.0",
    "electron": "2.0.7",
    "enzyme": "3.4.1",
    "enzyme-adapter-react-16": "1.2.0",
    "eslint": "5.2.0",
    "karma": "2.0.5",
    "karma-chai-plugins": "0.9.0",
    "karma-electron-launcher": "0.2.0",
    "karma-mocha": "1.3.0",
    "karma-sourcemap-loader": "0.3.7",
    "karma-teamcity-reporter": "1.1.0",
    "karma-webpack": "3.0.0",
    "mocha": "5.1.1",
    "react-test-renderer": "16.4.2",
    "sinon": "5.1.1",
    "sinon-chai": "3.2.0",
    "stylelint": "9.4.0",
    "xvfb-maybe": "0.2.1",
    "yo": "2.0.5"
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
